<../mkconfig

TARG=devwsys

XFILES=\
	alloc.$O\
	display.$O\
	event.$O\
	itrans.$O\
	keysym2ucs.$O\
	window.$O\
	wsys.$O\

OFILES=\
	alloc.$O\
	devdraw.$O\
	ixp_fsys.$O\
	ixp_srv.$O\
	keyboard.$O\
	latin1.$O\
	main.$O\
	mouse.$O\
	reqbuf.$O\
	window.$O\
	$SYSHOST-$OBJTYPE.$O\
	$LIBIXP\
	${XFILES:%=x11/%}\

XHFILES=\
	inc.h\
	keysym2ucs.h\
	x.h\

HFILES=\
	dat.h\
	fns.h\
	latin1.h\
	${XHFILES:%=x11/%}\

LIBS=	memlayer memdraw draw kern 9

BIN=$ROOT/$OBJDIR/bin

<$ROOT/mkfiles/mkone-$SHELLTYPE

# to include ./*.h files form x11/
CFLAGS=$CFLAGS -I.

# to remove .$O files from x11/
nuke:
	rm -f `echo $OS | sed 's,.,x11/*.&,g'`
